		}
	});
}

var carouselTimeDelay = 4000;
CAROUSELS = [];
function initContentCarousels() {
	var carousels = jQuery(".block-block .carousel");
	carousels.each(function(index,node) {
		var o = jQuery(this), items = o.find("li"), tray = o.find(".tray"), trayWidth = tray.width();

		items.css({position:"absolute",left:trayWidth}).each(function(index, node) {
			node.parentNode.index = index;
			if (index == 0) jQuery(this).css({left:0});

		});
		

		o.find(".items").css({width: trayWidth, left: 0, position:"absolute"});
		tray.css({left: 0, overflow:"hidden"});
		
		CAROUSELS[index] = {}
		CAROUSELS[index].target = this;
		CAROUSELS[index].isMoving = false;
		CAROUSELS[index].isLooping = false;
		CAROUSELS[index].currentItemIndex = Math.round(Math.random()*(items.length-1));	
		CAROUSELS[index].timerID = setTimeout("animateContentCarousel(" + index + ")", carouselTimeDelay);
		items.removeClass('on').eq(CAROUSELS[index].currentItemIndex).addClass('on').css({'z-index':2, left:0});
		
		o.find("li a").hover(function() {
			clearTimeout(CAROUSELS[index].timerID);
		}, function() {
			clearTimeout(CAROUSELS[index].timerID);
			CAROUSELS[index].timerID = setTimeout("animateContentCarousel(" + index + ")", carouselTimeDelay);
		});
	});
}

function animateContentCarousel(cindex) {
	var i = CAROUSELS[cindex].currentItemIndex + 1, t = CAROUSELS[cindex].target, o = jQuery(CAROUSELS[cindex].target),
		items = o.find("li"), trayWidth = o.find(".tray").width();
	
	if (i >= items.length) i = 0;
	
	items.eq(CAROUSELS[cindex].currentItemIndex).animate({left:-trayWidth}, 500, function() {
		jQuery(this).css({"z-index":1});
		CAROUSELS[cindex].currentItemIndex = i;
		clearTimeout(CAROUSELS[cindex].timerID);
		CAROUSELS[cindex].timerID = setTimeout("animateContentCarousel(" + cindex + ")", carouselTimeDelay);
	});
	items.eq(i).css({left:trayWidth,"z-index":2}).animate({left:0},500)
	
};
