					if (debug) console.log("slide prev: " + np.prev.find('img')[0].complete);
				}
				if (np.next != null) np.next.css({left: (opts.expandToFullWidth? o.width() : current.width()) + 'px', display:'block'});
				
				

				if (!opts.expandToFullWidth) {
					o.css({width:current.width() + 'px'});
					if (debug) console.log(current.css('width') + "; SLIDESHOW WIDTH: " + current.width());
				}
				var i = o.find('.slide.selected').prevAll('.slide').length;
				o.css({height:current.children()[0].clientHeight + 'px'}).find('.nav .btn-nav').removeClass('on').eq( i ).addClass('on');

				if (!opts.wrapSlideShow) {
					$('.srs .btn-prev').css({display: (i == 0)? 'none' : 'block'});
					$('.srs .btn-next').css({display: (i == o.find('.slide').length - 1)? 'none' : 'block'});
				}
			}	
			
			//slide to selected slide
			function slideTo(dir, fromTouch) {
				dir = (dir)? dir : 0;
				var easing = fromTouch? 'easeOutQuad' : 'easeInOutQuad';
				
				if ((dir < 0) && !opts.preventSlideOnRelease) {
					var c = np.prev.children(), iw = c.width(), ih = c.height();
					current.animate({left: np.prev.width() + 'px'}, t, easing); 
					
					if (np.prev.html() != np.next.html()) {
						np.next.animate({left: ( np.prev.width() + current.width()) + 'px'}, t, easing);
					}
					np.prev.animate({left: '0px'}, t, easing, 
						function() {
							setCurrent($(this));
							isMoving = false;									  
						}
					); 
				} else if ((dir > 0) && !opts.preventSlideOnRelease) {
					var c = np.next.children(), iw = c.width(), ih = c.height();
					
					current.animate({left: -current.width() + 'px'}, t, easing); 
					np.next.animate({left: '0px'}, t, easing, 
						function() {
							setCurrent($(this));
							isMoving = false;									  
						}
					); 
				} else {
					var c = current.children(), iw = c.width(), ih = c.height();
					
					np.prev.animate({left: np.prev.attr('x0') + 'px'}, t, easing); 
					np.next.animate({left: np.next.attr('x0') + 'px'}, t, easing);
					current.animate({left: current.attr('x0') + 'px'}, t, easing, 
						function() {
							isMoving = false;									  
						}
					); 
					
				}
				
				o.animate({width:iw, height:ih}, t, easing);

				if (opts.closeButtonSelector != null) {
					var r = 0.5*Math.round(iw / $(window).width() * 100)
					$(opts.closeButtonSelector).animate({right: (47.5-r) + '%'},t, easing);
				}
				
				if ((opts.slideDuration > 0) && !isTimerPaused) {
					clearTimeout(slideTimer);
					slideTimer = setTimeout(function() {slideTo(1)}, opts.slideDuration);
				}
			}
			
			function jumpTo() {
				if ($(this).hasClass('on')) {
					slideTo(0);
				} else {
					$(this).addClass('on').siblings('.btn-nav').removeClass('on');

					var i = $(this).prevAll('.btn-nav').length;
					s.eq(i).css({left: current.width(), display:'block'});
					np.next = s.eq(i);

					if (debug) console.log('img info:');
					if (debug) console.log(np.next.find('img'));
					
					slideTo(1);
					
				}
			}

			function destroy() {
				if (debug) console.log('destroying...');
				$(document).off('mousemove mouseup');
				document.body.removeEventListener('touchmove', touchmoveHandler);
				document.body.removeEventListener('touchend', touchendHandler);
				 
				$(window).off('resize');
			}
			
		});// end each
		
	}
	
	function getNextPrev(slist) {
		var	next, prev, pi = 0, ni = 0;
		slist.each(function(i,n) {
			var s = $(this);
			
			if (s.hasClass('selected')) {
				pi = i - 1;
				pi = (pi < 0)? slist.length - 1 : pi;
				
				prev = slist.eq(pi);
				
				ni = i + 1;
				ni = (ni >= slist.length)? 0 : ni;
				
				next = slist.eq(ni);
			}
		});
		
		return {next:next, prev:prev};
		
	}
	
