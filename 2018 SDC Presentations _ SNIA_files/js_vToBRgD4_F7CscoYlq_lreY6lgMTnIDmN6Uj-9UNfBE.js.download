function showGlanceValue(f) {
	var p = jQuery(this).parents('.glance'), t = p.find('.info strong'), icon = p.find('.icon');
	
	if (!icon.hasClass('on')) {
		icon.addClass('on');
	}

	t.html(formatAmount(Math.round(parseInt(t.attr('data-value'))*f)));
}

function formatAmount(val) {
	val = isNaN(val)? 0 : val;
	
	var str = '', v = val + '';
	
	if (v.length <= 3) {
		return v;
	} else {
		while (v != '') {
			var i = v.length - 3;
			if (i >= 0) {
				str = ',' + v.substr(i) + str;
				v = v.substr(0,i);
			} else {
				str = v + str;
				v = '';
			}
		}
		
		return (str.charAt(0) == ',')? str.substr(1) : str;
	}
}


